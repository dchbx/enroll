<% if @result.success? %>
  $("#<%= params[:feature][:key]%>-alert").html("<div class='alert alert-success alert-dismissible fade show' role='alert'><button type='button' class='close' data-dismiss='alert' aria-label='Close'><span aria-hidden='true'>&times;</span></button>Renewal created successfully.</div>").delay(3000).alert('clsoe');
<% else %>
  $("#<%= params[:feature][:key]%>-alert").html("<div class='alert alert-danger fade show' role='alert'><button type='button' class='close' data-dismiss='alert' aria-label='Close'><span aria-hidden='true'>&times;</span></button>Something went wrong. Renewal failed.</div>");
<% end %>