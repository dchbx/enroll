var containing_row = $("#<%= @row %>").closest('TR');
var $parent_row = containing_row;
if ( $('tr.child-row:visible').length > 0 ) {
 $('tr.child-row:visible').remove();
}

if ( $parent_row.next().hasClass('child-row') ) {
// Do Nothing
}
else {
 $parent_row.after('<tr class="child-row"><%= j render partial: "datatables/terminate_enrollment" %></tr>');
}
