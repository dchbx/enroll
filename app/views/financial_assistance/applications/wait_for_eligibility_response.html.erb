<% content_for :horizontal_status do %>
  <% if show_component(controller_name) %>
    <%= render :partial =>  'insured/families/horizontal_status.html.erb', locals: {step: 1} %>
  <% end %>
<% end %>
  <div class="container">
    <div id="new_person_wrapper">
      <div class="row">
        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-12">
          <%= render :partial => 'shared/left_nav.html.erb' %>
        </div>
        <div class="col-md-7">
          <div class="">
            <p class="alert alert-error hide"></p>
            <h1 class="darkblue">Waiting for your Eligibility results...</h1>
          </div>
        </div>

        
        <div class="col-lg-2 col-md-2 col-sm-2 col-xs-12">
          <%find_sep_url = find_sep_insured_families_path(consumer_role_id: @person.consumer_role.try(:id))%>
          <%= render  partial: 'shared/individual_progress', locals: {url:eligibility_results_financial_assistance_application_path} %>
        </div>
      </div>
    </div>
    <%= render partial: 'insured/plan_shoppings/help_with_plan'  %>
  </div>
