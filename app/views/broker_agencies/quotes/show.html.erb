<div class="module container broker_quote_tool">
  <div class="row" class="col-md-12">
   <input name='employee' value='50' type='hidden'>
   <input name='spouse' value='0' type='hidden'>
   <input name='domestic_partner' value='0' type='hidden'>
   <input name='child_under_26' value='0' type='hidden'>
    <%= form_tag(broker_agencies_quotes_root_path, method: :get) do  %>

    <h2>Quote Calculator</h2>

      <%= render partial: 'my_features' if @q.can_edit? %>

      <%= render partial: 'my_plans' if @q.can_edit? %>

      <% end %>
      <%= render partial: 'broker_agencies/quotes/panels/dental'  if @q.can_edit? %>
      <%= render partial: 'broker_agencies/quotes/panels/quote' %>
      <%= link_to "Back to Quotes", my_quotes_broker_agencies_quotes_path, class: 'btn btn-primary', data: { no_turbolink: true } %>
    </div>
  </div>
</div>


<script type="text/javascript">

$(document).on('ready page:load', function () {
    QuotePageLoad.set_select_health_plans(<%=raw @health_plan_quote_criteria %>)
    Quote.show_benefit_group("<%= @q.id%>", "<%= @quote_on_page.id%>")

    window.dental_relationship_benefits =  <%= raw @benefit_pcts_json %>
    window.dental_roster_premiums = <%= raw @dental_roster_premiums %>

});
</script>
