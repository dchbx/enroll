  }

  function callActiveEmployeeFilter(table_id) {
    EnrollDataTableInitializer.findTable(table_id);
    var $active_filter = $table.closest('.module').find('.Active-second-level a:contains("Employer-Sponsored-Coverage-Enrolled")');
