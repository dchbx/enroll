  }

  return {
    callAllAllFilter: callAllAllFilter,
    callAllIndividualFilter: callAllIndividualFilter,
    callAllEmployeeFilter: callAllEmployeeFilter,
    callActiveAllFilter: callActiveAllFilter,
    callActiveIndividualFilter: callActiveIndividualFilter,
    callActiveEmployeeFilter: callActiveEmployeeFilter,
    callAllFilter: callAllFilter
  };

} )( window );
