var AdminFamiliesDatatable = ( function( window, undefined ) {

  function setTab() {
    var tab = "Families";
    return tab;
  }

  function setBaseModel() {
    var base_model = "family";
    return base_model;
  }

  function callAllFilter(table_id) {
    EnrollDataTableInitializer.findTable(table_id);
    var $active_filter = $table.closest('.module').find('.first-level a:first:contains("All")');
    var filter_criteria = EnrollDataTableInitializer.setFilterCriteria(table_id, setTab(), EnrollDataTableInitializer.setSecondLevel($active_filter), undefined );
    var base_model = setBaseModel();
    EnrollDataTableInitializer.clickActiveFilter(table_id, $active_filter, filter_criteria, base_model);
  }

  function callAllAllFilter(table_id) {
    EnrollDataTableInitializer.findTable(table_id);
