#left-navigation {
  
  a:visited {
    color: #CBE6FF;
    font-size: 14px;
    font-weight: 600;
  }

  .applicant-nav a:visited {
    color: #0079C0;
    font-size: 14px;
  }

  .income-style {
    border-left: 7px solid #ffffff !important;
    a:visited
    {
      color: #ffffff;
    }
  }

  .active.applicant-nav a {
    font-weight: 600;
  }

  .active.indent-w {
    border-left: 7px solid #fff !important;
  }
  .white-font {
    color: #CBE6FF;
  }

  .activer.global-nav > a {
    color: #CBE6FF;
  }

  li:not(.activer) a:not(.disabled) {
    color:#0079C0;
  }

  .active {
    border-left: 7px solid #0079C0;
  }

  .global-nav.active {
    border-left: 7px solid #ffffff;
  }

  .disabled {
    background-color: white;
  }

  .global-nav.active a {
    color: #ffffff;
  }

  .active a {
    font-size: 14px;
    font-weight: 600;
  }

  .applicant-nav {
    background: transparent;
    a {
     background: #ffffff;
     font-weight: normal;
   }
  }

  li {
    background-color: #fff;
    border: 1px solid #c3c3c3;
  }

  li:first-child {
    border: 0px;
  }

  li:nth-child(n+2) {
    border-top: 1px solid #ffffff;
  }

  li.nactive-w {
    border-left: 7px solid #ffffff;
  }

  li:last-child {
    border-bottom: 1px solid #c3c3c3;
  }

  .applicant-check {
    color: #69A30D;
  }
  .cna {
    cursor: not-allowed !important;
    color:#909090 !important;
  }

  .cna {
    font-weight: normal;
  }
}